{
  "manifest_version": 2,

  "name": "Listen later for YouTube Music",
  "version": "0.1",
  "description": "Chrome extension to add a Listen later feature for YouTube Music",

  "icons": {
    "128": "logo128.png",
    "48": "logo48.png",
    "16": "logo16.png"
  },

  "content_scripts": [
    {
      "js": ["main.js"],
      "css": ["style.css"],
      "matches": ["https://music.youtube.com/*"]
    }
  ]
}
